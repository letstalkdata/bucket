                        bucket_setup_template $fclient $fnode $fmysql $fmssql $fk8s $fglust $fdtr $ftall
                        exit 0;
                        ;;
                    all )   fsall=1; 
                            ;;
                    * ) help_bucket_setup
                        exit 1
                esac
                shift
            bucket_setup $finit $fprofile $ftemplate $fsall
            ;;
        ls | list )    shift
                cmd1=$1
                case $cmd1 in
                    -h | --help )       help_bucket_list
                                        exit
                                        ;;
                    ns )  bucket_list_ns 
                        ;;
                    k8s )  shift
                            cmd2=$1
                            case $cmd2 in
                                -h | --help )   help_bucket_show_k8s
                                        exit
                                        ;;
                                * ) while [ "$1" != "" ]; do
                                        case $1 in
                                            -n | --namespace )  shift  
                                                    namespace=$1
                                                    ;;
                                            * ) help_bucket_show_k8s
                                                exit 1
                                        esac
                                        shift
                                    done
                                ;;
                            esac
                        shift
                        bucket_show_k8s $namespace 
                        ;;
                    k8sUI )  shift
                                cmd2=$1
                                case $cmd2 in
                                    -h | --help )   help_bucket_show_k8sui
                                            exit
                                            ;;
                                    * ) while [ "$1" != "" ]; do
                                            case $1 in
                                                -n | --namespace )  shift  
                                                    namespace=$1
                                                    ;;
                                                * ) help_bucket_show_k8sui
                                                    exit 1
                                            esac
                                            shift
                                        done
                                    ;;
                                esac
                            shift
                            bucket_show_k8sui $namespace
                        ;;
                    rope )  shift
                            cmd2=$1
                            case $cmd2 in
                                -h | --help )   help_bucket_list_rope
                                        exit
                                        ;;
                                * ) while [ "$1" != "" ]; do
                                        case $1 in
                                            -b | --bucket )  shift  
                                                    bucket=$1
                                                    ;;
                                            * ) help_bucket_list_rope
                                                exit 1
                                        esac
                                        shift
                                    done
                                ;;
                            esac
                        shift
                        bucket_list_rope $bucket 
                        ;;
                    node )  shift
                            cmd2=$1
                            case $cmd2 in
                                -h | --help )   help_bucket_list_node
                                        exit
                                        ;;
                                * ) while [ "$1" != "" ]; do
                                        case $1 in
                                            -n | --namespace )  shift  
                                                    namespace=$1
                                                    ;;
                                            * ) help_bucket_list_node
                                                exit 1
                                        esac
                                        shift
                                    done
                                ;;
                            esac
                        shift
                        bucket_list_node $namespace 
                        ;;
                    profile )  bucket_list_profile
                        ;;
                    * ) help_bucket_list
                        exit 1
                esac
            shift
            ;;
        exec )    shift
                cmd1=$1
                case $cmd1 in
                    -h | --help )   help_bucket_exec
                                    exit
                                    ;;
                    bash )  shift
                            isBash=1; 
                            nodename=$1 
                            bucket_exec_bash $nodename
                            ;;
                    code )  shift
                            cmd2=$1
                            case $cmd2 in
                                -h | --help )       help_bucket_exec_code
                                        exit
                                        ;;
                                * ) while [ "$1" != "" ]; do
                                        case $1 in
                                            -n | --namespace )  shift  
                                                    namespace=$1
                                                    ;;
                                            -b | --bucket )  shift  
                                                    bucket=$1
                                                    ;;
                                            -c | --code ) shift
                                                    codePath=$1
                                                    ;;
                                            -et |--execType )  shift  
                                                    execType=$1
                                                    ;;
                                            --onClient )  onClient=1
                                                    ;;
                                            * ) help_bucket_exec_code
                                                exit 1
                                        esac
                                        shift
                                    done
                                ;;
                            esac
                        shift
                        bucket_exec_code $namespace $bucket $codePath $execType $onClient 
                        ;;
                    * ) help_bucket_exec
                        exit 1
                esac
            shift
            ;;
        file )  shift
                cmd1=$1
                case $cmd1 in
                    -h | --help )   help_bucket_file
                                    exit
                                    ;;
                    pull )  shift
                            cmd2=$1
                            case $cmd2 in
                                -h | --help )       help_bucket_file_pull
                                        exit
                                        ;;
                                * ) while [ "$1" != "" ]; do
                                        case $1 in
                                            -s | --source )  shift  
                                                    source=$1
                                                    ;; 
                                            -d | --destination ) shift
                                                    destination=$1
                                                    ;;
                                            * ) help_bucket_file_pull
                                                exit 1
                                        esac
                                        shift
                                    done
                                ;;
                            esac
                        shift
                        bucket_file_pull $source $destination
                        ;;
                    push )  shift
                            cmd2=$1
                            case $cmd2 in
                                -h | --help )       help_bucket_file_push
                                        exit
                                        ;;
                                * ) while [ "$1" != "" ]; do
                                        case $1 in
                                            -n | --namespace )  shift  
                                                    namespace=$1
                                                    ;;
                                            -b | --bucket )  shift  
                                                    bucket=$1
                                                    ;;
                                            -f | --file ) shift
                                                    file=$1
                                                    ;;  
                                            -et |--execType )  shift  
                                                    execType=$1
                                                    ;;
                                            * ) help_bucket_file_push
                                                exit 1
                                        esac
                                        shift
                                    done
                                ;;
                            esac
                        shift
                        bucket_file_push $namespace $bucket $file $execType
                        ;;
                    * ) help_bucket_file
                        exit 1
                esac
            shift
            ;;        
        * )         help_bucket
                    exit 1
    esac
    shift
done